public class PythagoreanTriplet {

	public static void main(String[] args) 
  {
		    int a,b,c;
		    int product=0;
		    for (a=1;a<1000;a++)
        {
		  for (b=1;b<1000;b++)
      {
		    for (c=1;c<1000;c++)
        {
		      if (a+b+c==1000)
          {
		        if((a*a)+(b*b)==(c*c))
            {
		          product=a*b*c;
		          break;
		         }
		       }
		      }
		  }
		    }
		System.out.println(product);

		}
		
	}
