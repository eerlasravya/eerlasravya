public class SquareDifference {
	
	static long SquareDiff(long n){
		 
		long l, k, m;
		    l = (n * (n + 1) * (2 * n + 1)) /6;
		    k = (n * (n + 1)) / 2;
		    k = k * k;
		    m = Math.abs(l - k);
		    return m;
		 
		}
		public static void main(String s[])
		{
		    long n = 100;
		    System.out.println(SquareDiff(n));    
		     
		}
		}

